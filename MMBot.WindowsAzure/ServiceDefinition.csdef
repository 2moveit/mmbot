<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="MMBot.WindowsAzure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WorkerRole name="MMBot.WorkerRole" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="http" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <LocalResources>
    </LocalResources>
    <ConfigurationSettings>
    </ConfigurationSettings>
    <Runtime>
      <Environment>
        <Variable name="MMBOT_ROUTER_ENABLED" value="true"/>
        <Variable name="MMBOT_ROUTER_PORT" value="1234"/>
        <Variable name="MMBOT_JABBR_HOSTNAME" value="https://jabbr.net"/>
      </Environment>
    </Runtime>
  </WorkerRole>
</ServiceDefinition>